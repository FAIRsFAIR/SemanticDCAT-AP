@prefix dash:     <http://datashapes.org/dash#> .
@prefix dcat:     <http://www.w3.org/ns/dcat#> .
@prefix dct:      <http://purl.org/dc/terms/> .
@prefix sh:       <http://www.w3.org/ns/shacl#> .
@prefix mod:      <http://purl.org/mod#> .
@prefix obo:      <http://purl.obolibrary.org/obo/> .
@prefix vcard:    <http://www.w3.org/2006/vcard/ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc:       <http://purl.org/dc/elements/1.1/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

mod:FsFSemanticArtefactShape a sh:NodeShape ;
  sh:targetClass mod:SemanticArtefact ;
  sh:property [
    sh:path dct:title ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:license ;
    sh:class: dct:LicenseDocument ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:identifier ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:accessRights ;
    sh:class: dct:RightsStatement ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:creator ;
    sh:node :AgentShape ;
    sh:minCount 1 ;
    dash:editor dash:BlankNodeEditor ;
  ], [
    sh:path dct:created ;
    sh:datatype xsd:dateTime ;
    sh:minCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ], [
    sh:path dct:description ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    dash:editor dash:TextAreaEditor ;
  ], [
    sh:path dcat:contactPoint ;
    sh:nodeKind sh:IRI ;
    #sh:class vcard:Kind ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path owl:versionIRI ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:modified ;
    sh:datatype xsd:dateTime ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ],  [
    sh:path dcat:keyword ;
    sh:nodeKind sh:Literal ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
  ], [
    sh:path mod:acronym ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
  ], [
    sh:path dcat:landingPage ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:publisher ;
    sh:node :AgentShape ;
    sh:minCount 1 ;
    dash:editor dash:BlankNodeEditor ;
  ], [
    sh:path dct:subject ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    dash:editor dash:TextFiledEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dc:type ;
    sh:nodeKind sh:IRI ;
    #sh:class rdfs:Class ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ], [
    sh:path dct:issued ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ],  [
    sh:path dcat:theme ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ] .

:AgentShape a sh:NodeShape ;
  sh:targetClass foaf:Agent ;
  sh:property [
    sh:path foaf:name;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:maxCount  1 ;
    dash:editor dash:TextFieldEditor ;
  ] .

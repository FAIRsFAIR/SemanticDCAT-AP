  sh:targetClass semdcat:saTemporal ;
  sh:property [
    sh:path semdcat:temporalStart ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:dateTime ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
  ], [
    sh:path semdcat:temporalEnd ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:dateTime ;
    sh:maxCount 1 ;
    dash:editor dash:DatePickerEditor ;
    dash:viewer dash:LiteralViewer ;
] .
